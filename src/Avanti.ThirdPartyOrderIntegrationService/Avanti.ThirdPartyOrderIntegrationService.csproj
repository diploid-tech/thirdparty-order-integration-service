<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <DebugType>full</DebugType>
    <AssemblyName>Avanti.ThirdPartyOrderIntegrationService</AssemblyName>
    <PackageId>OrderService</PackageId>
    <RootNamespace>Avanti.ThirdPartyOrderIntegrationService</RootNamespace>
    <TreatWarningsAsErrors>True</TreatWarningsAsErrors>
    <TreatSpecificWarningsAsErrors />
    <Nullable>enable</Nullable>
    <NoWarn>$(NoWarn);CA1014</NoWarn>
    <EnableNETAnalyzers>true</EnableNETAnalyzers>
    <AnalysisMode>AllEnabledByDefault</AnalysisMode>
    <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="**\*.sql" />
  </ItemGroup>
  <ItemGroup>
    <None Update="appsettings.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="avanti-service.yaml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Avanti.Core.EventStream.Azure" Version="1.1.1-beta-gf07e36835c" />
    <PackageReference Include="Avanti.Core.KeyStore.Azure" Version="1.1.0-beta-g2423dcb9fd" />
    <PackageReference Include="Avanti.Core.Microservice" Version="1.5.1-beta-gc4a86dbd42" />
    <PackageReference Include="Avanti.Core.Http" Version="1.1.0-beta-gd59e1f1654" />
    <PackageReference Include="Avanti.Core.QueueProcessor" Version="1.0.0-beta-gffc752067a" />
    <PackageReference Include="Avanti.Core.RelationalData.PostgreSQL" Version="1.3.0-beta-g94311fc46b" />
  </ItemGroup>
</Project>
